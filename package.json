{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.1",
    "antd": "^3.26.15",
    "classnames": "^2.2.6",
    "echarts": "^4.7.0",
    "lint-staged": "^10.0.7",
    "moment": "^2.24.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.1.1",
    "video-react": "^0.14.1",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
